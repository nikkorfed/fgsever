              tr
                td Ремонт ТНВД
                td от 5 000 ₽
              tr
                td Замена клапана VANOS
                td от 20 000 ₽
              tr
                td Замена датчика уровня масла двигателя
                td от 2 000 ₽
              tr
                td Ремонт системы охлаждения
                td от 500 ₽
              tr
                td Замена головки блока цилиндров BMW #{series.modelDesc}
                td от 6 000 ₽
              tr
                td Замена форсунок
                td от 300 ₽
              tr
                td Ремонт форсунок
                td от 1500 ₽
              tr
                td Замена термостата
                td от 5 500 ₽
              tr
                td Компьютерная диагностика двигателя
                td 1 500 ₽
              tr
                td Механическая диагностика ДВС BMW #{series.modelDesc}
                td от 1 500 ₽
