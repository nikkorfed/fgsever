extends /layout.pug

block variables
  - var name = 'main_high main_6-series';
  - var section = 'Вид работ';
  - var title = 'Диагностика и ремонт ДВС в BMW&nbsp;6&nbsp;серии';
  - var models = 'G32, F06/F12/F13, E63/E64';
  - var seoTitle = 'Диагностика и ремонт ДВС в BMW 6 серии (' + models + ') в Москве | Автосервис BMW «FGSEVER»';
  - var subtitle = 'Услуги по компьютерному выявлению и устранению ошибок в работе двигателя автомобилей BMW 6&nbsp;серии в Москве. Мы выполним все необходимые работы по обслуживанию вашего автомобиля и ответим на все интересующие вас вопросы.';
  - var path = {'/services/': 'Услуги', '/services/maintenance/': 'Техническое обслуживание BMW', '/services/maintenance/6-series/': '6&nbsp;серия', '': 'Диагностика и ремонт ДВС'};
  - var button = ['popup', '#maintenance-appointment', 'Записаться'];

block content
  +main({ name, path, section, title, subtitle, button, models })
  include /maintenance/description/engine-repair
  section.prices
    .wrapper
      .titles
        .overtitle Об услуге
        h2 Стоимость
      .block
        .table-wrapper
          .table
            table
              tr.table-header
                td Услуга
                td Стоимость
              tr 
                td Диагностика и ремонт ДВС
                td Рассчитывается индивидуально, в зависимости от сложности ремонта
  include /maintenance/photos/engine-repair
  include /about/advantages
  +about
  include /contacts
