extends /layout.pug

block variables
  - var name = 'main_high main_1-series'
  - var section = 'Услуга';
  - var title = 'Техническое обслуживание BMW&nbsp;1&nbsp;серии';
  - var models = 'F20/F21, E81/E82/E87/E88';
  - var seoTitle = 'Техническое обслуживание (ТО) BMW 1 серии (' + models + ') в Москве | Автосервис BMW «FGSEVER»';
  - var subtitle = 'Услуги по техническому обслуживанию (ТО) BMW 1&nbsp;серии в Москве — компьютерная и общая диагностика, замена масел и технических жидкостей, тормозных дисков и колодок, цепи ГРМ, ремонт и замена ДВС, мойка радиаторов, ремонт рулевой рейки.';
  - var path = {'/services/': 'Услуги', '/services/maintenance/': 'Техническое обслуживание BMW', '': '1&nbsp;серия'};
  - var button = ['#maintenance-calculator', 'Рассчитать стоимость'];

block content
  +main({ name, path, section, title, subtitle, button, models })
  include /maintenance/calculator
  include /maintenance/services
  include /contacts
