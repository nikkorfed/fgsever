extends /layout.pug

block variables
  - var name = 'main_high main_x2';
  - var section = 'Вид работ';
  - var title = 'Чистка и мойка впускного коллектора в BMW&nbsp;X2';
  - var models = 'F39';
  - var seoTitle = 'Чистка и мойка впускного коллектора в BMW X2 (' + models + ') в Москве | Автосервис BMW «FGSEVER»';
  - var subtitle = 'Услуги по тщательной чистке и мойке впускного коллектора автомобилей BMW X2 в Москве. Мы выполним все необходимые работы по обслуживанию вашего автомобиля и ответим на все интересующие вас вопросы.';
  - var path = {'/services/': 'Услуги', '/services/maintenance/': 'Техническое обслуживание BMW', '/services/maintenance/x2/': 'X2', '': 'Чистка и мойка впускного коллектора'};
  - var button = ['popup', '#maintenance-appointment', 'Записаться'];

block content
  +main({ name, path, section, title, subtitle, button, models })
  include /maintenance/description/intake-manifold-cleaning
  section.prices
    .wrapper
      .titles
        .overtitle Об услуге
        h2 Стоимость
      .block
        .table-wrapper
          .table
            table
              tr.table-header
                td Услуга
                td Стоимость
              tr 
                td Чистка и мойка впускного коллектора
                td Рассчитывается индивидуально, в зависимости от сложности работ
  include /maintenance/photos/intake-manifold-cleaning
  include /about/advantages
  +about
  include /contacts
