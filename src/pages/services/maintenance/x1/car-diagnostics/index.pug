extends /layout.pug

block variables
  - var name = 'main_high main_x1';
  - var section = 'Вид работ';
  - var title = 'Диагностика автомобиля в BMW&nbsp;X1';
  - var models = 'F48, E84';
  - var seoTitle = 'Диагностика автомобиля в BMW X1 (' + models + ') в Москве | Автосервис BMW «FGSEVER»';
  - var subtitle = 'Услуги по общей диагностике и выявлению основных неисправностей автомобилей BMW X1 в Москве. Мы выполним все необходимые работы по обслуживанию вашего автомобиля и ответим на все интересующие вас вопросы.';
  - var path = {'/services/': 'Услуги', '/services/maintenance/': 'Техническое обслуживание BMW', '/services/maintenance/x1/': 'X1', '': 'Диагностика автомобиля'};
  - var button = ['popup', '#maintenance-appointment', 'Записаться'];

block content
  +main({ name, path, section, title, subtitle, button, models })
  include /maintenance/description/car-diagnostics
  section.prices
    .wrapper
      .titles
        .overtitle Об услуге
        h2 Стоимость
      .block
        .table-wrapper
          .table
            table
              tr.table-header
                td Услуга
                td Стоимость
              tr
                td Общая диагностика автомобиля (включая компьютерную диагностику, диагностику подвески и течи)
                td 2 100 ₽
  include /maintenance/photos/car-diagnostics
  include /about/advantages
  include /about  
  include /contacts
