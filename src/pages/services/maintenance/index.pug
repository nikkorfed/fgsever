extends /layout.pug

block variables
  - var name = 'main_maintenance';
  - var section = 'Направление';
  - var title = 'Техническое обслуживание BMW';
  - var seoTitle = title + ' в Москве | Автосервис BMW «FGSEVER»';
  - var subtitle = 'Услуги по техническому обслуживанию (ТО) автомобилей BMW в Москве — компьютерная и общая диагностика, замена масел и технических жидкостей, тормозных дисков и колодок, цепи ГРМ, ремонт и замена ДВС, мойка радиаторов, ремонт рулевой рейки.';
  - var path = {'/services/': 'Услуги', '': title};

block content
  +main(name, path, section, title, subtitle)
  include /maintenance/calculator
  include /maintenance/models
  include /contacts
